<Window x:Class="LeagueAutoAccept.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:LeagueAutoAccept"
        mc:Ignorable="d"
        Loaded="Window_Loaded"
        Title="League of Legends Auto Accept" Height="214.831" Width="272.713"
        ResizeMode="NoResize"
        Background="{DynamicResource WindowBackgroundBrush}">
    
    <Grid Margin="-2,0,4,0">
        <Button x:Name="startButton" Click="startButton_Click" Content="Start" Margin="175,151,0,0" HorizontalAlignment="Left" Width="75" Height="18" VerticalAlignment="Top"/>
        <TextBox x:Name="logBox" HorizontalAlignment="Left" Height="159" Margin="10,10,0,0" TextWrapping="Wrap" IsReadOnly="True" Text="" VerticalAlignment="Top" Width="137"/>
        <Button x:Name="aboutButton" Content="About" FontSize="8" HorizontalAlignment="Left" Height="17" Margin="220,5,0,0" VerticalAlignment="Top" Width="35" RenderTransformOrigin="1.057,0.183" Click="aboutButton_Click"/>
        <!-- <ComboBox x:Name="championsList"  IsEditable="False" IsReadOnly="True" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Width="120"/> !-->
    </Grid>
</Window>
