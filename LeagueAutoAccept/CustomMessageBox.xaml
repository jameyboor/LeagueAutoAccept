<Window x:Class="LeagueAutoAccept.CustomMessageBox"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:LeagueAutoAccept"
        mc:Ignorable="d"
        Title="CustomMessageBox" Height="118.142" Width="324.558"
        Background="{DynamicResource WindowBackgroundBrush}">
    <Grid>
        <Label x:Name="textLabel" Content="Text" HorizontalContentAlignment="Center" HorizontalAlignment="Left" Margin="10,0,0,70" VerticalAlignment="Bottom"/>
        <Button x:Name="button" Content="Ok" HorizontalAlignment="Left"  Margin="124,0,0,0" VerticalAlignment="Bottom" Width="75" Click="button_Click_1"/>

    </Grid>
</Window>
